{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": ["jest"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jest/no-disabled-tests": "warn",
        "jest/no-focused-tests": "error",
        "jest/no-identical-title": "error",
        "jest/prefer-to-have-length": "warn",
        "jest/valid-expect": "error"
      },
    "globals": {
        "document": true,
        "window": true,
    },
    "env": {
        "jest/globals": true
    }
}